{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "app/routes/**/*.tsx",
    "react-router-vite/entry.*.tsx",
    "react-router-vite/plugin.ts",
    "scripts/*.ts",
    "tests/**/*.ts",
    "app/lib/reader/parser-worker.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "react-router-vite/**/*.{ts,tsx}",
    "scripts/**/*.ts",
    "tests/**/*.ts"
  ],
  "ignore": ["app/types/**/*.d.ts", "local/**"],
  "ignoreDependencies": [
    "@tailwindcss/typography",
    "@types/*",
    "puppeteer",
    "tailwindcss",
    "canvas",
    "isbot"
  ],
  "vite": {
    "config": ["vite.config.ts"]
  },
  "vitest": {
    "config": ["vite.config.ts"]
  }
}
